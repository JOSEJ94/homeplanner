  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "bump-major": "npm version major --no-git-tag-version && (cd android && bundle exec fastlane bump) && (cd ios && bundle exec fastlane bump)",
    "bump-minor": "npm version minor --no-git-tag-version && (cd android && bundle exec fastlane bump) && (cd ios && bundle exec fastlane bump)",
